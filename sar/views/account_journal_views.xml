<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="account_journal_form_view_inherit">
        <field name="name">account.journal.form.view.inherit</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <page name="bank_account" position="inside">
                <group name="sar" string="SAR">
                    <group col="2">
                        <field name="cai_ventas" options="{'no_create':True}"/>
                        <field name="cai_nota_credito" options="{'no_create':True}"/>
                        <field name="cai_nota_debito" options="{'no_create':True}"/>
                       <field name="cai_retencion" options="{'no_create':True}"/>
                    </group>
                </group>
            </page>
        </field>
    </record>
</odoo>


